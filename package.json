{
  "name": "info-rut",
  "version": "5.0.2",
  "description": "Get person or enterprise from a valid RUT or full name",
  "main": "src",
  "types": "src/types.d.ts",
  "scripts": {
    "lint:js": "eslint . --fix",
    "format:js": "prettier-standard '{src,ts}/**/*.js'",
    "lint:ts": "tslint --project . --fix '{src,ts}/**/*.ts'",
    "format:ts": "prettier-standard '{src,ts}/**/*.ts'",
    "lint": "npm run lint:js && npm run lint:ts",
    "format": "npm run format:js && npm run format:ts",
    "test": "nyc mocha --exit",
    "snyk-protect": "snyk protect"
  },
  "engines": {
    "node": ">=10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lgaticaq/info-rut.git"
  },
  "keywords": [
    "rut"
  ],
  "author": "Leonardo Gatica <lgatica@protonmail.com> (https://about.me/lgaticaq)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lgaticaq/info-rut/issues"
  },
  "homepage": "https://github.com/lgaticaq/info-rut#readme",
  "dependencies": {
    "cheerio": "^1.0.0-rc.3",
    "fuse.js": "^3.4.5",
    "object.fromentries": "^2.0.0",
    "rut.js": "^1.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "8.1.0",
    "@commitlint/config-conventional": "8.1.0",
    "@semantic-release/changelog": "3.0.4",
    "@semantic-release/git": "7.0.16",
    "@semantic-release/github": "5.4.2",
    "@semantic-release/npm": "5.1.13",
    "chai": "4.2.0",
    "eslint": "6.1.0",
    "eslint-config-jsdoc-strict": "2.0.0",
    "eslint-config-prettier": "6.0.0",
    "eslint-config-standard": "13.0.1",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsdoc": "15.8.1",
    "eslint-plugin-node": "9.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-security": "1.4.0",
    "eslint-plugin-standard": "4.0.0",
    "husky": "3.0.4",
    "lint-staged": "9.2.2",
    "mocha": "6.2.0",
    "nock": "10.0.6",
    "npm-github-config": "2.0.0",
    "nyc": "14.1.1",
    "nyc-config-common": "1.0.1",
    "prettier-standard": "9.1.1",
    "semantic-release": "15.13.21",
    "snyk": "1.217.0",
    "tslint": "5.18.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-config-standard": "8.0.1",
    "typescript": "3.5.3"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "standard",
      "jsdoc-strict",
      "plugin:promise/recommended",
      "plugin:security/recommended"
    ],
    "plugins": [
      "promise",
      "security"
    ],
    "rules": {
      "no-console": [
        "error"
      ],
      "require-await": [
        "error"
      ]
    }
  },
  "eslintIgnore": [
    "coverage",
    "example.js"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "{src,test}/**/*.js": [
      "eslint --fix",
      "prettier-standard",
      "git add"
    ],
    "{src,test}/**/*.ts": [
      "tslint --project . --fix",
      "prettier-standard",
      "git add"
    ]
  },
  "tonicExampleFilename": "example.js",
  "renovate": {
    "automerge": "minor",
    "extends": [
      "config:js-lib"
    ]
  },
  "nyc": {
    "extends": "nyc-config-common"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release": {
    "extends": "npm-github-config"
  },
  "snyk": true
}
